\beta=110
\]

\end_inset


\end_layout

\begin_layout Standard

\lang spanish
\begin_inset Formula 
\[
\frac{I_{C_{1}}}{\beta}=I_{B_{1}}
\]

\end_inset


\end_layout

\begin_layout Standard

\lang spanish
\begin_inset space ~
\end_inset


\end_layout

\begin_layout Standard

\lang spanish
\begin_inset Formula 
\[
I_{B_{2}}=I_{C_{1}}\,\,\,\,\,\,\,\Longrightarrow\,\,\,\,\,\,\,I_{B_{2}}=\beta I_{B_{1}}
\]

\end_inset


\end_layout

\begin_layout Standard

\lang spanish
\begin_inset space ~
\end_inset


\end_layout

\begin_layout Standard

\lang spanish
\begin_inset Formula 
\[
\frac{I_{C_{2}}}{\beta}=I_{B_{2}}\,\,\,\,\,\,\Longrightarrow\,\,\,\,\,\,\,\,I_{C_{2}}=\beta^{2}I_{B_{1}}
\]

\end_inset


\end_layout

\begin_layout Standard

\lang spanish
\begin_inset space ~
\end_inset


